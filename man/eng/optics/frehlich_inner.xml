<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE> frehlich_inner  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE> May, 2006 </DATE>
  <SHORT_DESCRIPTION name="frehlich_inner"> construct an Frehlich inner scale.  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>[in]=frehlich_inner(frehlich_inner_scale)  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
 <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>frehlich_inner_scale  </PARAM_NAME>
  <PARAM_DESCRIPTION>
          <SP>: the frehlich inner scale in metre.</SP>
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>in  </PARAM_NAME>
  <PARAM_DESCRIPTION>
          <SP>: the output inner scale;</SP>
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>  
 </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
  <P>
    set the frehlich inner scale. 
  </P>
  </DESCRIPTION>
  <EXAMPLE><![CDATA[
	pl=power_law(0.1,1e-6);
	in=frehlich_inner(0.001);
	ps=power_spectrum(pl,in);
 ]]></EXAMPLE>
