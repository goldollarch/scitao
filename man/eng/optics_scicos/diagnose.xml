	<ModuleName>diagnose</ModuleName>
	
	<ModuleIcon>diagnose.gif</ModuleIcon>
	
	<Library>Optics</Library>
	
	<Description>This block output the diagnose information.</Description>
	
	<DialogBoxes>
		<Items>
			<Item>
				<ItemTitle>Set diagnose file parameters</ItemTitle>
				<ItemContent>(default value)</ItemContent>
			</Item>
			<Item>
				<ItemTitle>file name</ItemTitle>
				<ItemContent>diagnose</ItemContent>
			</Item>
			<Item>
				<ItemTitle>Accept herited events 0/1</ItemTitle>
				<ItemContent>0</ItemContent>
			</Item>			
		</Items>
		<ItemExplain>
			<Explain>
				<Subject>file name : </Subject>
				<Content>input the diagnose information filename.</Content>
			</Explain>
			<Explain>
				<Subject>Accept herited events 0/1 : </Subject>
				<Content>set the herited events, "0" represents no,"1" represents yes.</Content>
			</Explain>
		</ItemExplain>
	</DialogBoxes>
	
	<DefaultProperties>
		<Property>
			<Subject>always active:</Subject>
			<Content>no</Content>
		</Property>
	</DefaultProperties>
	
	<ExampleDiagrams> 
		<ExampleIcon>diagnose_em.gif</ExampleIcon>
	</ExampleDiagrams>
	
	<InterfacingFunction>macros/DIAGNOSE.sci</InterfacingFunction>
	
	<ComputationalFunction>src/scicos/diagnose.c</ComputationalFunction>
	
